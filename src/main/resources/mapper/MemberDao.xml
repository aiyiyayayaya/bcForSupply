        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mindata.blockchain.core.dao.BC.MemberDao">

    <insert id="insertMember" parameterType="com.mindata.blockchain.core.entity.BC.Member">
        INSERT INTO
        member(create_time, update_time, app_id, group_id, ip, name)
        values (#{createTime}, #{updateTime},  #{appId}, #{groupId}, #{ip}, #{name})
    </insert>

    <delete id="deleteMember">
        delete from member where app_Id = #{appId}
    </delete>

